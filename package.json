{
  "name": "node-crud-api",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./dist && babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --inspect=0.0.0.0:9229 --transpile-only --ignore node_modules -r tsconfig-paths/register src/server.ts",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create -n",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "migration:show": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:show",
    "migration:rollback": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert",
    "migration:drop-create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:drop && ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "seed:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create -c seed -n",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -c seed",
    "up": "docker-compose -f docker-compose.development.yml up",
    "stop": "docker-compose -f docker-compose.development.yml stop",
    "down": "docker-compose -f docker-compose.development.yml down",
    "prod:up": "docker-compose up",
    "prod:stop": "docker-compose stop",
    "prod:down": "docker-compose down"
  },
  "dependencies": {
    "@types/node": "^15.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "class-validator-jsonschema": "^3.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "routing-controllers-openapi": "^3.0.0",
    "swagger-ui-express": "^4.1.6",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.29",
    "typeorm-typedi-extensions": "^0.4.1",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/node": "^7.13.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.3",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/multer": "^1.4.5",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.2.1"
  },
  "engines": {
    "node": "12.x"
  }
}
