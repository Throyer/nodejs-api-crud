{
  "name": "node-crud-api",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --inspect --transpile-only --ignore node_modules src/server.ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "migration:generate": "ts-node-dev ./node_modules/typeorm/cli.js migration:generate",
    "migration:create": "ts-node-dev ./node_modules/typeorm/cli.js migration:create -n",
    "migration:run": "ts-node-dev ./node_modules/typeorm/cli.js migration:run",
    "migration:show": "ts-node-dev ./node_modules/typeorm/cli.js migration:show",
    "migration:revert": "ts-node-dev ./node_modules/typeorm/cli.js migration:revert",
    "migration:restart": "ts-node-dev ./node_modules/typeorm/cli.js schema:drop && ts-node-dev ./node_modules/typeorm/cli.js migration:run",
    "prestart": "rm -rf ./dist && tsc"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0-alpha.6",
    "typeorm": "^0.2.28",
    "typescript": "^4.0.3",
    "@types/node": "^14.14.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.4",
    "ts-node-dev": "^1.0.0",
  }
}